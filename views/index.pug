                              form#form1.fw_form_fw_form(data-fw-form-id='fw_form', method='post', data-fw-ext-forms-type='contact-forms')
                                input(type='hidden', name='fwf', value='fw_form')
                                input#_nonce_271fca0b2bb618b7f84815c4d944dab4(type='hidden', name='_nonce_271fca0b2bb618b7f84815c4d944dab4', value='214f76bc4f')
                                input(type='hidden', name='_wp_http_referer', value='/website/mf-drone-warfare/')
                                input(type='hidden', name='fw_ext_forms_form_type', value='contact-forms')
                                input(type='hidden', name='fw_ext_forms_form_id', value='6f91151150af7bca8c532047defd694a')
                                .wrap-forms
                                  .fw-row
                                  .fw-row
                                    .fw-col-xs-12.fw-col-sm-6.form-builder-item
                                      .field-radio.input-styled
